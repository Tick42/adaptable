		"husky": "^0.14.0",
		"jest": "^22.0.2",
		"lint-staged": "^7.1.3",
		"lodash.camelcase": "^4.3.0",
		"postcss-url": "^8.0.0",
		"prettier": "^1.13.4",
		"prompt": "^1.0.0",
		"replace-in-file": "^3.0.0-beta.2",
		"rimraf": "^2.6.1",
		"rollup": "^0.59.2",
