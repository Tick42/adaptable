{
  "name": "adaptableblotter-react",
  "version": "0.2.34",
  "description": "",
  "keywords": [],
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Samed Duzcay <samedduzcay@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist && rimraf node_modules/adaptableblotter/dist/App_Scripts/*.ts",
    "copy-styles": "copyfiles -f node_modules/adaptableblotter/dist/App_Scripts/Styles/stylesheets/adaptableblotter-style.css dist/styles && copyfiles -f node_modules/adaptableblotter/dist/App_Scripts/Styles/fonts/* dist/fonts",
    "build": "tsc --module commonjs && npm run rollup && npm run copy-styles && typedoc --out docs --target es6 --theme minimal --mode file src",
    "gh-pages": "node scripts/publish-gh-pages.js",
    "prepublish": "npm run build",
    "postpublish": "npm run gh-pages",
    "postbuild": "node scripts/postbuild.js",
    "rollup": "node --max-old-space-size=8192 node_modules/rollup/bin/rollup -c rollup.config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jest": "^22.0.0",
    "@types/node": "^10.0.3",
    "@types/react": "^16.4.12",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "copyfiles": "^2.1.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.0",
    "jest": "^22.0.2",
    "lint-staged": "^7.1.3",
    "lodash.camelcase": "^4.3.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.13.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.59.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "ts-jest": "^22.0.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "adaptableblotter": "^2.7.3",
    "ag-grid": "^18.1.2",
    "ag-grid-enterprise": "^18.1.1",
    "ag-grid-react": "^18.1.0"
  },
  "peerDependencies": {
    "mathjs": "^5.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "gitHead": "8570abd9ef3ab90c7a037f774e55588e7b35b621"
}
