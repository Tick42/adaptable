	"scripts": {
		"release": "npm run release:patch",
		"prerelease": "npm run build && release pre && npm run registry-publish",
		"release:patch": "npm run build && release patch && npm run registry-publish",
		"release:minor": "npm run build && release minor && npm run registry-publish",
		"release:major": "npm run build && release major && npm run registry-publish",
		"registry-publish": "cp package.json dist && npm publish dist --dry-run",
		"build": "rimraf dist && npm run tsc && npm run copy-assets",
		"tsc": "tsc --outDir dist",
		"copy-assets": "cp -R ../adaptableblotter/dist/fonts ./dist/fonts/ && cp -R ../adaptableblotter/dist/stylesheets ./dist/stylesheets/ && cp -R ../adaptableblotter/dist/themes ./dist/themes/"
	}
