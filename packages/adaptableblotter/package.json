  "scripts": {
    "build-apidoc": "typedoc --options ./typedocconfig.ts",
    "pretty": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "registry-publish": "npm run build-package-json && npm run update-version-file && npm publish dist",
    "registry-publish-canary": "npm run build-package-json && npm run update-version-file && npm publish --tag canary dist",
    "rmdist": "rimraf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-deps": "rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && npm i && rimraf node_modules/@types/react-native",
    "build-prod": "npm run clean-deps && npm run rmdist && npm run tsc && npm run build-package-json && npm run sass && npm run copy-assets && cp LICENSE.md dist",
    "build-package-json": "node ../../scripts/buildPackageJson",
    "update-version-file": "node ../../scripts/updateVersionFile",
    "sass": "node-sass ./App_Scripts/index.scss ./dist/index.css && ../../node_modules/.bin/postcss ./dist/index.css --config ../../ --output ./dist/index.css && node-sass ./App_Scripts/base.scss ./dist/base.css && ../../node_modules/.bin/postcss ./dist/base.css --config ../../ --output ./dist/base.css && node-sass ./App_Scripts/themes/light.scss ./dist/themes/light.css && ../../node_modules/.bin/postcss ./dist/themes/light.css --config ../../ --output ./dist/themes/light.css && node-sass ./App_Scripts/themes/dark.scss ./dist/themes/dark.css && ../../node_modules/.bin/postcss ./dist/themes/dark.css --config ../../ --output ./dist/themes/dark.css ",
    "copy-assets": "cp -R ./App_Scripts/fonts ./dist/ && cp README.md ./dist && cp LICENSE.md ./dist",
    "tsc": "tsc --outDir dist",
    "tscv": "tsc --version",
    "build": "npm run build-prod",
    "openfin": "node openfin-launcher.js",
    "configserver": "ts-node configserver/configserver.ts --configfolder ./tmp | bunyan"
  },
