  "scripts": {
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-abvendor": "webpack --config webpack.vendor.js",
    "build-themes": "node postcss-scopify.config.js",
    "build-dev": "webpack --config webpack.dev.js",
    "build-prod": "increase-memory-limit && npm run prebuild && webpack --config webpack.prod.js -p && npm run build-ts",
    "prepublish": "npm run build-prod",
    "build-ts": "tsc --outDir ./dist/App_Scripts",
    "build-ts-watch": "tsc --outDir ./dist/App_Scripts --watch",
    "dev-hypergrid": "cross-env arg=hypergrid webpack-dev-server --config webpack.dev.js --content-base Harness/Hypergrid/",
    "dev-aggrid": "cross-env arg=aggrid webpack-dev-server --config webpack.dev.js --content-base Harness/agGrid/",
    "dev-aggrid-react": "cross-env arg=aggrid-react webpack-dev-server --config webpack.dev.js --content-base Harness/agGridReact/",
    "dev-react": "cross-env arg=react webpack-dev-server --config webpack.dev.js --content-base Harness/React/",
    "openfin": "node openfin-launcher.js",
    "configserver": "ts-node configserver/configserver.ts --configfolder ./tmp | bunyan"
  },
