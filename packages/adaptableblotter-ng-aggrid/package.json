{
  "name": "@adaptabletools/adaptableblotter-ng-aggrid",
  "version": "0.0.0",
  "publishConfig": {
    "registry": "https://registry.adaptabletools.com"
  },
  "description": "Angular version of the powerful data-agnostic HTML5 datagrid add-on that sits on top of the agGrid component and provides all the rich functionality that advanced users expect from their grids and blotters",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptableTools/adaptableblotter.git",
    "directory": "packages/adaptableblotter-ng-aggrid"
  },
  "bugs": {
    "url": "https://github.com/AdaptableTools/adaptableblotter/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "^8.2.0",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/core": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "ag-grid-angular": "^21.0.0",
    "ag-grid-community": "^21.0.0",
    "ag-grid-enterprise": "^21.0.0",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mathjs": "^5.1.1",
    "ng-packagr": "^5.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "rimraf": "^2.6.2",
    "rxjs": "~6.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "ng": "ng",
    "link-core": "npm run --prefix ./projects/adaptableblotter-angular-aggrid link-core",
    "start": "npm run link-core && node --max-old-space-size=4096 ./node_modules/.bin/ng serve --prod --sourceMap=false",
    "clean-deps": "rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && npm i && npm run clean-deps-projects",
    "clean-deps-projects": "rimraf projects/adaptableblotter-angular-aggrid/yarn.lock && rimraf projects/adaptableblotter-angular-aggrid/package-lock.json && rimraf projects/adaptableblotter-angular-aggrid/node_modules && npm --prefix projects/adaptableblotter-angular-aggrid/ i",
    "build": "npm run clean-deps && rimraf dist && npm run link-core && node --max-old-space-size=4096 ./node_modules/.bin/ng build adaptableblotter-angular-aggrid && npm run build-package-json && npm run sass && npm run copy-assets",
    "test": "ng test",
    "lint": "ng lint",
    "tsc": "tsc",
    "e2e": "ng e2e",
    "pretty": "prettier --write \"packages/**/*.{js,jsx,ts,tsx,json,css,scss}\" --fix",
    "registry-publish": "npm run build-package-json && npm run update-version-file && npm publish dist/adaptableblotter-angular-aggrid",
    "registry-publish-canary": "npm run build-package-json && npm run update-version-file && npm publish --tag canary dist/adaptableblotter-angular-aggrid",
    "build-package-json": "node ./scripts/buildPackageJson",
    "update-version-file": "node ./scripts/updateVersionFile",
    "sass": "node-sass ./src/index.scss ./dist/adaptableblotter-angular-aggrid/index.css && ../../node_modules/.bin/postcss ./dist/adaptableblotter-angular-aggrid/index.css --config ../../ --output ./dist/adaptableblotter-angular-aggrid/index.css && node-sass ./src/base.scss ./dist/adaptableblotter-angular-aggrid/base.css && ../../node_modules/.bin/postcss ./dist/adaptableblotter-angular-aggrid/base.css --config ../../ --output ./dist/adaptableblotter-angular-aggrid/base.css && node-sass ./src/themes/light.scss ./dist/adaptableblotter-angular-aggrid/themes/light.css && ../../node_modules/.bin/postcss ./dist/adaptableblotter-angular-aggrid/themes/light.css --config ../../ --output ./dist/adaptableblotter-angular-aggrid/themes/light.css && node-sass ./src/themes/dark.scss ./dist/adaptableblotter-angular-aggrid/themes/dark.css && ../../node_modules/.bin/postcss ./dist/adaptableblotter-angular-aggrid/themes/dark.css --config ../../ --output ./dist/adaptableblotter-angular-aggrid/themes/dark.css",
    "copy-assets": "cp .npmignore ./dist/adaptableblotter-angular-aggrid && cp -R ../adaptableblotter/App_Scripts/fonts ./dist/adaptableblotter-angular-aggrid/fonts/ && cp README.md ./dist/adaptableblotter-angular-aggrid/"
  }
}
